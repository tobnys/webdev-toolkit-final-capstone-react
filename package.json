    "chai-http": "^3.0.0",
    "mocha": "^3.5.0",
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.4.0",
    "cpr": "^2.0.0",
    "enzyme": "^3.1.0",
    "faker": "^3.1.0",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.5.4",
    "react-scripts": "0.9.0",
    "react-test-renderer": "^15.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
